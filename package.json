{
  "name": "MyWebsiteFrontEnd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lintfix": "eslint --fix --ext .js,.vue src",
    "mockJson": "json-server --watch mock/data.json",
    "mockJS": "json-server mock/generate.js "
  },
  "dependencies": {
    "@moefe/vue-aplayer": "^2.0.0-beta.5",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "fingerprintjs2": "^2.0.6",
    "highlight.js": "^9.15.6",
    "js-md5": "^0.7.3",
    "normalize.css": "^8.0.1",
    "vee-validate": "^2.2.4",
    "vue": "^2.6.10",
    "vue-markdown": "^2.2.4",
    "vue-matomo": "^3.7.0-2",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.13",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^3.6.95",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-vue": "^5.2.2",
    "faker": "^4.1.0",
    "json-server": "^0.14.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^2.1.0",
    "style-loader": "^0.23.1",
    "vue-cli-plugin-vuetify": "^0.4.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "semi": 1,
      "brace-style": [
        2,
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": 2,
      "indent": 1,
      "comma-dangle": [
        1,
        "always"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
